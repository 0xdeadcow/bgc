.PHONY: clean

clean:
	$(MAKE) -C functions $@
	$(MAKE) -C intro $@
	$(MAKE) -C pointers $@
	$(MAKE) -C varstat $@
	$(MAKE) -C arrays $@
	$(MAKE) -C strings $@
	$(MAKE) -C structs $@

pristine:
	$(MAKE) -C functions $@
	$(MAKE) -C intro $@
	$(MAKE) -C pointers $@
	$(MAKE) -C varstat $@
	$(MAKE) -C arrays $@
	$(MAKE) -C strings $@
	$(MAKE) -C structs $@
